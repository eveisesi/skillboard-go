type Character @goModel(model: "github.com/eveisesi/skillz.Character") {
    id: Uint64!
    name: String!
    corporationID: Uint!
    allianceID: Uint
    factionID: Uint
    securityStatus: Float
    gender: String!
    birthday: Time!
    title: String
    bloodlineID: Uint!
    raceID: Uint!
}

extend type User {
    character: Character!
}

extend type CharacterImplant {
    character: Character!
}
